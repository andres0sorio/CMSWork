	<!-- Declare a context that contain applcation -->
    	<xc:Context url="http://localhost.localdomain:1972">
		
		<!-- Declare a xdaqTtuConfig application -->
                <xc:Application class="xdaqTtuConfig" id="10" network="local"/>
		
		<!-- Shared object library that contains the xdaqTtuConfig implementation -->		
		<xc:Module>${XDAQ_ROOT}/xdaq-app/xdaqTtuConfig/lib/linux/x86_RedHat/libxdaqTtuConfig.so</xc:Module>
        </xc:Context>

</xc:Partition>


